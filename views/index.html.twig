{% extends "layout.html.twig" %}

{% block title %}Home{% endblock %}

{% block content %}
{% for article in articles %}
<article>
    <h2><a class="productTitle" href="{{ path('product', { 'id': product.id }) }}">{{ product.title }}</a></h2>
    <p>{{ product.price }}</p>
</article>
{% endfor %}
{% endblock %}